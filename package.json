{
  "name": "collimator",
  "version": "0.0.0",
  "description": "Reflection & Introspection for PostgreSQL Databases",
  "repository": "git+https://github.com/radify/collimator",
  "main": "lib/collimator.js",
  "author": "Radify",
  "license": "BSD-3-Clause",
  "devDependencies": {
    "babel-core": "^6.2.1",
    "babel-plugin-rewire": "1.0.0-beta-2",
    "babel-preset-es2015": "^6.1.18",
    "cz-conventional-changelog": "^1.1.5",
    "del": "^2.1.0",
    "gulp": "gulpjs/gulp#4.0",
    "gulp-babel": "^6.1.0",
    "gulp-istanbul": "^0.10.3",
    "gulp-jasmine": "^2.2.1",
    "gulp-jscs": "^3.0.2",
    "gulp-markdox2": "^1.9.94",
    "isparta": "^4.0.0",
    "require-dir": "^0.3.0"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "env": {
      "test": {
        "plugins": [
          "rewire"
        ]
      }
    }
  },
  "build": {
    "paths": {
      "src": "src",
      "spec": "spec",
      "dist": "lib",
      "docs": "docs",
      "build": "build",
      "coverage": "coverage",
      "sql": "src/**/*.sql"
    },
    "watch": {
      "paths": "src"
    }
  },
  "jshintConfig": {
    "esnext": true
  },
  "jscsConfig": {
    "preset": "google",
    "jsDoc": {
      "checkAnnotations": "jsdoc3"
    },
    "esnext": true,
    "disallowParenthesesAroundArrowParam": true,
    "requireArrowFunctions": true,
    "requireShorthandArrowFunctions": true,
    "requireTemplateStrings": true
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "bluebird": "^3.1.1",
    "callsite": "^1.0.0",
    "ramda": "^0.19.1"
  }
}
